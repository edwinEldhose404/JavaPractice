import java.util.Scanner;

class sumNum 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		int oddsum = 0;
		int evensum = 0;
		
		for (int i = a ; i<b;i++)
		{
			if (i%2==0)
			{
				evensum+=i
			}
			else
			{
				oddsum+=i
			}
		}
		System.out.println(evensum + " " + oddsum);
	}
}
