import java.util.Scanner;

class multiNum 
{
	public static void main(String[] args) 
	{
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		
		long oddsum = 0;
		long evensum = 0;
		
		for (int i = a ; i<b;i++)
		{
			if (i%2==0)
			{
				evensum*=i;
			}
			else
			{
				oddsum*=i;
			}
		}
		System.out.println(evensum + " " + oddsum);
	}
}